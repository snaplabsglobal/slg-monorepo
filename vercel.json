{
    "version": 2,
    "buildCommand": "cd apps/ls-landing && npm install && npm run build",
    "installCommand": "cd apps/ls-landing && npm install",
    "framework": "nextjs",
    "outputDirectory": "apps/ls-landing/.next",
    "name": "ledger-snap-v1",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/$1"
        }
    ]
}